<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ericliudeveloper.mvpevent" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.ericliudeveloper.mvpevent.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.ericliudeveloper.mvpevent.SetNameActivity"
            android:label="@string/title_activity_second" >
        </activity>
        <activity
            android:name="com.ericliudeveloper.mvpevent.DisplayInfoActivity"
            android:label="@string/title_activity_do_nothing" >
        </activity>
    </application>

</manifest>
